/*github markdown style from Mou*/
.doit {margin:0;padding:0}
div.doit {
  font:13.34px helvetica,arial,freesans,clean,sans-serif;
  color:black;
  line-height:1.4em;
  padding: 0.7em;
}
.doit p {
  margin:1em 0;
  line-height:1.5em;
}
.doit table {
  font-size:inherit;
  font:100%;
  margin:1em;
}
.doit table th{border-bottom:1px solid #bbb;padding:.2em 1em;}
.doit table td{border-bottom:1px solid #ddd;padding:.2em 1em;}
.doit input[type=text],.doit input[type=password],.doit input[type=image],.doit textarea{font:99% helvetica,arial,freesans,sans-serif;}
.doit select,.doit option{padding:0 .25em;}
.doit optgroup{margin-top:.5em;}
.doit pre,.doit code{font:12px Monaco,"Courier New","DejaVu Sans Mono","Bitstream Vera Sans Mono",monospace;}
.doit pre {
  margin:1em 0;
  font-size:12px;
  background-color:#eee;
  border:1px solid #ddd;
  padding:5px;
  line-height:1.5em;
  color:#444;
  overflow:auto;
  -webkit-box-shadow:rgba(0,0,0,0.07) 0 1px 2px inset;
  -webkit-border-radius:3px;
  -moz-border-radius:3px;border-radius:3px;
}
.doit pre code {
  padding:0;
  font-size:12px;
  background-color:#eee;
  border:none;
}
.doit code {
  font-size:12px;
  background-color:#f8f8ff;
  color:#444;
  padding:0 .2em;
  border:1px solid #dedede;
}
.doit img{border:0;max-width:100%;}
.doit abbr{border-bottom:none;}
.doit a{color:#4183c4;text-decoration:none;}
.doit a:hover{text-decoration:underline;}
.doir a code,.doit a:link code,.doit a:visited code{color:#4183c4;}
.doit h2,.doit h3{margin:1em 0;}
.doit h1,.doit h2,.doit h3,.doit h4,.doit h5,.doit h6{border:0;}
.doit h1{font-size:170%;border-top:4px solid #aaa;padding-top:.5em;margin-top:1.5em;}
.doit h1:first-child{margin-top:0;padding-top:.25em;border-top:none;}
.doit h2{font-size:150%;margin-top:1.5em;border-top:4px solid #e0e0e0;padding-top:.5em;}
.doit h3{margin-top:1em;}
.doit hr{border:1px solid #ddd;}
.doit ul{margin:1em 0 1em 2em;}
.doit ol{margin:1em 0 1em 2em;}
.doit ul li,.doit ol li{margin-top:.5em;margin-bottom:.5em;}
.doit ul ul,.doit ul ol,.doit ol ol,.doit ol ul{margin-top:0;margin-bottom:0;}
.doit blockquote{margin:1em 0;border-left:5px solid #ddd;padding-left:.6em;color:#555;}
.doit dt{font-weight:bold;margin-left:1em;}
.doit dd{margin-left:2em;margin-bottom:1em;}
@media screen and (min-width: 768px) {
        div.doit {
          width: 748px;
          margin:10px auto;
        }
      }
